<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="@dimen/setting_dialog_width"
    android:layout_height="match_parent"
    android:background="@drawable/setting_gps_background"
    android:orientation="vertical"
    android:padding="@dimen/layout_content_left_right" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="38dp"
        android:text="车辆已注册成功，请等待审核"
        android:textColor="#6c6c6c"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/textView_shenghe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/textView1"
        android:layout_below="@+id/textView1"
        android:layout_marginTop="36dp"
        android:background="#eeeeee"
        android:text="@string/setting_gps_but_tip"
        android:textColor="#d24848"
        android:textSize="@dimen/layout_content_text_size" />

</RelativeLayout>