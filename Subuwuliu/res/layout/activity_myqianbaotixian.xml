<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/aaaaa"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical" >

    <include
        android:id="@+id/setting_title"
        layout="@+layout/layout_title" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:layout_marginTop="40dp"
        android:background="#ffffff"
        tools:ignore="UselessLeaf" >

        <TextView
            android:id="@+id/TextViewa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="20dp"
            android:text="保障财产，你只能使用"
            android:textColor="#AEAEAE"
            android:textSize="14dp"
            tools:ignore="ContentDescription,HardcodedText" />

        <TextView
            android:id="@+id/TextViewb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/TextViewa"
            android:text="本人本地"
            android:textColor="#C90000"
            android:textSize="14dp"
            tools:ignore="HardcodedText" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/TextViewb"
            android:text="银行卡！"
            android:textColor="#AEAEAE"
            android:textSize="14dp"
            tools:ignore="HardcodedText" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/huibaise"
        tools:ignore="UselessLeaf" >

        <TextView
            android:id="@+id/TextViewc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="账号姓名"
            android:textColor="#AEAEAE"
            android:textSize="16dp"
            tools:ignore="HardcodedText" />

        <EditText
            android:id="@+id/tixian_name"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/TextViewc"
            android:background="@color/huibaise"
            android:maxLength="24"
            android:textColor="#AEAEAE"
            android:textSize="16dp"
            tools:ignore="HardcodedText,TextFields,LabelFor" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#AEAEAE" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        tools:ignore="UselessLeaf" >

        <TextView
            android:id="@+id/TextViewd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="提现账号"
            android:textColor="#AEAEAE"
            android:textSize="16dp"
            tools:ignore="HardcodedText" />

        <EditText
            android:id="@+id/tixian_kahao"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/TextViewd"
            android:background="#ffffff" 
            android:hint="请输入卡号"
         	 android:inputType="number"
            android:textColor="#AEAEAE"
            android:maxLength="22"
            android:textSize="16dp"
            tools:ignore="HardcodedText,TextFields,LabelFor" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#AEAEAE" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:background="#ffffff"
        tools:ignore="UselessLeaf" >

        <TextView
            android:id="@+id/TextViewe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="20dp"
            android:text="保障财产，每日提现上限"
            android:textColor="#AEAEAE"
            android:textSize="14dp"
            tools:ignore="ContentDescription,HardcodedText" />

        <TextView
            android:id="@+id/TextViewf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/TextViewe"
            android:text="15000"
            android:textColor="#C90000"
            android:textSize="14dp"
            tools:ignore="HardcodedText" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/TextViewf"
            android:text="元！"
            android:textColor="#AEAEAE"
            android:textSize="14dp"
            tools:ignore="HardcodedText" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:background="#ffffff"
        tools:ignore="UselessLeaf" >

        <TextView
            android:id="@+id/TextViewg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="20dp"
            android:text="可提现金额  "
            android:textColor="#000000"
            android:textSize="14dp"
            tools:ignore="ContentDescription,HardcodedText" />

        <TextView
            android:id="@+id/TextViewh"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/TextViewg"
            android:text="0.00"
            android:textColor="#C90000"
            android:textSize="14dp"
            tools:ignore="HardcodedText" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/TextViewh"
            android:text=" 元"
            android:textColor="#000000"
            android:textSize="14dp"
            tools:ignore="HardcodedText" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="15dp"
        android:background="#AEAEAE" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        tools:ignore="UselessLeaf" >

        <TextView
            android:id="@+id/TextViewj"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="提现金额"
            android:textColor="#AEAEAE"
            android:textSize="16dp"
            tools:ignore="HardcodedText" />

        <EditText
            android:id="@+id/tixian_jine"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/TextViewj"
            android:background="#ffffff"
            android:digits="123456789.0"
            android:hint="0.00"
          android:inputType="number"
            android:textColor="#AEAEAE"
            android:textSize="16dp"
            tools:ignore="HardcodedText,TextFields,LabelFor" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:text="元"
            android:textColor="#AEAEAE"
            tools:ignore="HardcodedText" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#AEAEAE" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        tools:ignore="UselessLeaf" >

        <TextView
            android:id="@+id/TextViewl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="登录密码"
            android:textColor="#AEAEAE"
            android:textSize="16dp"
            tools:ignore="HardcodedText" />

        <EditText
            android:id="@+id/tixian_mima"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/TextViewl"
            android:background="#ffffff"
            android:hint="请输入登录密码"
            android:inputType="textPassword"
            android:textColor="#AEAEAE"
            android:textSize="16dp"
            tools:ignore="HardcodedText,TextFields,LabelFor" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#AEAEAE" />

    <Button
        android:id="@+id/tixian_tijiao"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="20dp"
        android:background="@color/qianhuise"
        android:text="确认提现"
        tools:ignore="HardcodedText" />

</LinearLayout>